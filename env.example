# Supabase 설정
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key
SUPABASE_TABLE_NAME=bible_chunks

# Google Generative AI 설정 (임베딩 및 LLM용)
GOOGLE_API_KEY=your_google_api_key
EMBEDDING_MODEL=models/gemini-embedding-001
EMBEDDING_DIMENSION=1536  # output_dimensionality 파라미터로 설정할 차원 (1536 권장, ivfflat 인덱스 지원)

# FastAPI 설정
API_HOST=0.0.0.0
API_PORT=8000

# CORS 설정
# 로컬 개발: http://localhost:3000
# Vercel 배포: https://your-project.vercel.app (실제 Vercel 배포 URL로 변경)
# 쉼표로 구분하여 여러 URL 추가 가능
ALLOWED_ORIGINS=http://localhost:3000,https://your-project.vercel.app

# LLM 설정 (Google Gemini)
LLM_MODEL=gemini-pro

